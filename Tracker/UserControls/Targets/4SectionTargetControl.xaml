<UserControl x:Class="Tracker.UserControls.Targets._4SectionTargetControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tracker.UserControls.Targets"
             mc:Ignorable="d" VerticalAlignment="Top" HorizontalAlignment="Left"
             d:DesignHeight="250" d:DesignWidth="250">
    <Grid>
        <Viewbox>
            <Grid>
                <Ellipse Fill="Gray" Width="250" Height="250"/>
                <Ellipse Fill="Green" Width="200" Height="200"/>
                <Ellipse Fill="Azure" Width="150" Height="150"/>
                <Ellipse Fill="Black" Width="100" Height="100"/>
            </Grid>
        </Viewbox>
    </Grid>
</UserControl>
