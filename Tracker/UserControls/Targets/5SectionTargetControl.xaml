<UserControl x:Class="Tracker.UserControls.Targets._5SectionTargetControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Tracker.UserControls.Targets"
             mc:Ignorable="d" Height="300" Width="300"
             MouseLeftButtonDown="Grid_MouseLeftButtonDown"
             MouseMove="Grid_MouseMove" VerticalAlignment="Top" HorizontalAlignment="Left"
             MouseLeftButtonUp="Grid_MouseLeftButtonUp" MouseDoubleClick="UserControl_MouseDoubleClick" LostFocus="UserControl_LostFocus">
    <Grid>
        <Grid.RenderTransform>
            <TranslateTransform x:Name="controlTranslateTransform"/>
        </Grid.RenderTransform>
        <Border x:Name="grid">
            <Viewbox>
                <Grid>
                    <Ellipse x:Name="el1" Fill="Red" Width="300" Height="300"/>
                    <Ellipse Fill="Gray" Width="250" Height="250"/>
                    <Ellipse Fill="Green" Width="200" Height="200"/>
                    <Ellipse Fill="Azure" Width="150" Height="150"/>
                    <Ellipse x:Name="el2" Fill="Black" Width="100" Height="100"/>
                </Grid>
            </Viewbox>
        </Border>
    </Grid>
</UserControl>
